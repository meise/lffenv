config tinc-net intracity
	option enable 1

	option generate_keys 1
	option Name NONAME
	option Mode switch
	option DirectOnly 1
	option Forwarding off
	list ConnectTo aftermath
	option Compression 10
	option GraphDumpFile /tmp/log/tinc.intracity.dot

config tinc-net private
	option enable 0

	option generate_keys 1
	option Name NONAME
	option Mode switch
	option Compression 10
	option GraphDumpFile /tmp/log/tinc.private.dot

config tinc-host NONAME
	option net intracity
	option Port 1194

config tinc-host aftermath
	option net intracity
	list Address 193.34.68.223
	option Port 1194
